datasource db {
  provider = "postgresql"
  url      = env("DB_URL_DEV")
  // url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-1.1.x", "linux-musl"]
}

model User_Cardeal {
  id       String  @id @default(uuid())
  email    String  @unique
  password String
  admin    Boolean @default(false)
}

model User_Invoice {
  id       String  @id @default(uuid())
  email    String  @unique
  password String
  admin    Boolean @default(false)
}

model User_Feedback {
  id       String  @id @default(uuid())
  email    String  @unique
  password String
  admin    Boolean @default(false)
}
